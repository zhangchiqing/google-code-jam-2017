{
  "parserOptions": {
    "ecmaVersion": 5,
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "env": {
    "jasmine": true,
    "node": true,
    "mocha": true,
    "amd": true,
    "browser": true,
    "builtin": true
  },
  "globals": {
    "$": true,
    "chrome": true
  },
  "rules": {
    "block-scoped-var": 2,
    "complexity": [
      2,
      7
    ],
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": [
      2,
      "smart"
    ],
    "max-depth": [
      2,
      5
    ],
    "max-len": [
      2,
      {"code": 90, "ignoreComments": true}
    ],
    "max-statements": [
      2,
      10
    ],
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-undef": 2,
    "no-with": 2,
    "no-dupe-keys": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ]
  }
}
